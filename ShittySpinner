        bloodline = value;
        local eye = {
            [1]="band",
            [2]="Eye"
        }
        
        local kg = {
            [1] = "spin",
            [2] = "kg1"     --spin kg1
        }

        local kg2 = {
            [1] = "spin",
            [2] = "kg2"     --spin kg2
        }
        
          --Saver
        
        local resetspins = {
            [1]="band",
            [2]="\128"
        }
        
        game:GetService("Players").localPlayer.startevent:FireServer(unpack(resetspins))

        

    end)
    
    Main:NewButton("Snip Quirk (kg1)", "(hell op)", function(v)
        ------------------------------------------------------------------------------
        local eye = {
            [1]="band",
            [2]="Eye"
        }
        
        local kg = {
            [1] = "spin",
