    Main:NewToggle("Restore Spins once Depleted" , "(10)", function(b)
 getgenv().autospin = b
    while true do
        wait()
        if not getgenv().autospin then return end
            wait()
                if game:GetService("Players").localPlayer.statz.spins.Value == 0 then --if spins 0 then saves you
                    game:GetService("TeleportService"):Teleport(4616652839, LocalPlayer) 
        end
     end

end)

Main:NewButton("Save Progress (DO IF YOU GOT SOMETHING YOU DIDNT SELECT)" , "(10)", function(b)
    getgenv().autospin = b
       while true do
           wait()
           if not getgenv().autospin then return end
               wait()
               local eye = {
                [1]="band",
                [2]="Eye"
            }
            game:GetService("Players").localPlayer.startevent:FireServer(unpack(eye)) 
        end
   
   end)

Main:NewLabel("For InGame")
Main:NewButton("Tp To Private Serv", "Copy Discord server to clipboard", function()
    game:GetService("TeleportService"):Teleport(4601350214, LocalPlayer)
end)



    Main:NewLabel("Made By JMaxeyy")
    Main:NewButton("Discord", "Copy Discord server to clipboard", function()
        setclipboard(tostring("https://discord.gg/aNhdfpv9ee"))
        bc = BrickColor.new("Dark stone Gray")
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
                Text = "Copied Discord To Clipboard";
                Font = Enum.Font.Cartoon;
                Color = bc.Color;
                FontSize = Enum.FontSize.Size96
    })
    end)



--Shindo script For Prium Hub

if game.PlaceId == 7811552487 or game.PlaceId == 4601350214 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Universal RPG Fucker", "Synapse")
  

    local Main = Window:NewTab("Player")
    local PlayerSection = Main:NewSection("Player")

      
    local Main = Window:NewTab("Main")
    local MainS = Main:NewSection("Main")

    ------Player--------
    PlayerSection:NewSlider("WalkSpeed", "Makes you walk faster ig", 300, 16, function(v) -- 500 (MaxValue) | 0 (MinValue)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)
    PlayerSection:NewSlider("JumpForce", "you already know waht tf it does", 500, 16, function(v) -- 500 (MaxValue) | 0 (MinValue)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end)
    ------Player--------

    



    ------MAIN--------


    


    MainS:NewToggle("Auto Tai,Hp", "(hell op)", function(v)

        getgenv().autospin = v
            while true do
        wait()
            if not getgenv().autospin then return end
            local args = {
                [1] = "addstat",
                [2] = "health",
                [3] = 1
            }
            
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            local args = {
                [1] = "addstat",
                [2] = "taijutsu",
                [3] = 1
            }
            
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            

    
end
end)

    MainS:NewToggle("Auto Rank", "(hell op)", function(v)

                getgenv().autospin = v
                    while true do
                wait()
                    if not getgenv().autospin then return end
                    if game:GetService("Players").LocalPlayer.statz.lvl.lvl.Value == 1000 then
                        local args = {
                            [1] = "maxlvlpres"
                        }
                        
                        game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
                        
                    end

            
        end
    end)
end

